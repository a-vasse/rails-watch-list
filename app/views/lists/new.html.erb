<div class="container mt-3">
  <div class="form-card m-2">
    <%= simple_form_for @list do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file %>
      <%= f.submit class: 'btn btn-light m-2' %>
      <%= link_to 'Back to Main', lists_path, class: 'btn btn-light m-2' %>
    <% end %>
  </div>
</div>
